@model Category


<div class="container-form">

    <h2>Crea una nuova Foto</h2>

    <div>
        <form method="post" asp-asp-controller="Category" asp-action="Create" enctype="multipart/form-data">
            @Html.AntiForgeryToken()
            <div>
                <label asp-for="Name">Nome</label>
                <input asp-for="Name" />
                <span asp-validation-for="Name"></span>
            </div>
            <button type="submit">Crea ora!</button>
        </form>
    </div>

</div>
